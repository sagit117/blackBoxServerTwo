{
    "server": {
        "port": 8081,
        "headers": [
            {
                "key": "Access-Control-Allow-Origin",
                "value": "*"
            },
            {
                "key": "Access-Control-Allow-Origin",
                "value": "GET,PUT,POST,DELETE,PATCH,OPTIONS"
            },
            {
                "key": "Access-Control-Allow-Headers",
                "value": "Content-Type"
            }
        ],
        "useCompression": true,
        "bodyParser": {
            "limit": "50mb",
            "extended": false,
            "parameterLimit": 50000
        }
    },
    "logger": {
        "messages": {
            "error": "red.bold.bgWhiteBright",
            "info": "blue.bold.bgWhiteBright",
            "warning": "yellow.bold.bgWhiteBright",
            "dateTime": "bgBlack.bold.whiteBright",
            "text": "whiteBright"
        }
    }
}
