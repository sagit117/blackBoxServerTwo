{
    "server": {
        "port": 8081,
        "headers": [
            {
                "key": "Access-Control-Allow-Origin",
                "value": "*"
            },
            {
                "key": "Access-Control-Allow-Origin",
                "value": "GET,PUT,POST,DELETE,PATCH,OPTIONS"
            },
            {
                "key": "Access-Control-Allow-Headers",
                "value": "Content-Type"
            }
        ]
    },
    "logger": {
        "messages": {
            "error": "red.bold",
            "info": "blue",
            "warning": "yellow",
            "dateTime": "bgBlack.bold.white"
        }
    }
}
